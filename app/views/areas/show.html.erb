

    <div class="contentmain">
      <div class="row skylinebg content">
        <h1 class="citytitle">Exeter Escorts</h1>
        <div class="large-12 columns padding2v">
					<% @escorts.each_slice(3) do |row_escort| %>
					<div class="row padding3b">
						<% row_escort.each do |escort| %>
					    <div class="large-4 columns content1">
					    	<%= link_to escort_path(escort) do %>
						      <div class="large-3 columns singlelisting backgroundcover" style="background: url('<%= "http://escortdirectory-uk.com/" + escort.pictures.first %>');">
						      </div>
						      <div class="large-9 columns profileinfo singlelisting">
						        <div class="content1">
						          <h4 class="listingname"><%= escort.name %>, <%= escort.age %></h4>
						          <p class="font08">My name is black hippo and I am much black hippo tbh asdioajsd asijd  sdij sd </p>
						        </div>
						        <p class="viewprofile text-center"><span class="font08">V</span>IEW PROFIL<span class="font08">E</span></p>
						      </div>
					      <% end %>
					    </div>
						<% end %>
					</div>
					<% end %>
        </div>
      </div>
      </div>

        <script type="text/javascript">
            (function() {

                /* matchHeight example */

                $(function() {


                  $(function() {
                    $('.singlelisting').matchHeight();
                  });


                });

            })();
        </script>